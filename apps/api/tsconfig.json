{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES2015",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "typeRoots": ["./src/types"],
    "types": ["express-session", "express"]
  },
  "include": ["src", "src/types"],
  "exclude": ["node_modules"]
}
