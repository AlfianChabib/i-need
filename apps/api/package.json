{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.development.local cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "start": "dotenv -e .env.production.local -- node -r tsconfig-paths/register dist/index.js",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\" --max-warnings 0",
    "clean": "rm -rf dist",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "tsx src/seed.ts",
    "generate": "prisma generate",
    "prebuild": "npm run generate",
    "predev": "npm run generate",
    "studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "@react-email/render": "^0.0.17",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "express": "^4.18.3",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "react": "^18.3.1",
    "resend": "^4.0.0",
    "ts-node-dev": "^2.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.15",
    "@types/supertest": "^6.0.2",
    "esbuild": "^0.20.1",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prisma": "^5.18.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
